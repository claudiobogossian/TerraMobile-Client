<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/geoform_lay"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:background="@color/cinza1"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical"
    android:weightSum="10" >

	<ScrollView
	    android:id="@+id/geoform_lay1"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent"
	    android:layout_weight="10"
	    android:focusableInTouchMode="true" >

		<LinearLayout
		    android:id="@+id/geoform_lay11"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:baselineAligned="false"
		    android:focusableInTouchMode="true"
		    android:orientation="vertical"
		    android:weightSum="10" >

			<LinearLayout
			    android:id="@+id/geoform_lay111"
			    android:layout_width="fill_parent"
			    android:layout_height="fill_parent"
			    android:layout_weight="0"
			    android:baselineAligned="false"
			    android:focusableInTouchMode="true"
			    android:orientation="vertical" >

				<LinearLayout
				    android:id="@+id/geoform_lay1111"
				    android:layout_width="fill_parent"
				    android:layout_height="fill_parent"
				    android:layout_weight="0"
				    android:background="@color/cinza3"
				    android:orientation="vertical"
				    android:padding="10dp" >

					<TextView
					    android:id="@+id/geoform_text1"
					    android:layout_width="wrap_content"
					    android:layout_height="wrap_content"
					    android:layout_gravity="center"
					    android:text="Inserir Dados"
					    android:textAppearance="?android:attr/textAppearanceMedium" />

				</LinearLayout>
			</LinearLayout>

			<LinearLayout
			    android:id="@+id/trechoconfig_lay112"
			    android:layout_width="fill_parent"
			    android:layout_height="fill_parent"
			    android:layout_gravity="center_horizontal"
			    android:layout_marginLeft="4dp"
			    android:layout_marginRight="4dp"
			    android:layout_weight="10"
			    android:background="@color/cinza1"
			    android:baselineAligned="false"
			    android:focusableInTouchMode="true"
			    android:orientation="vertical"
			    android:paddingBottom="5dp"
			    android:paddingTop="5dp" >
				
			    <LinearLayout
	    android:id="@+id/geoform_lay_ph"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="0"
	    android:paddingTop="3dp"
	    android:weightSum="1" >

		<Button
		    android:id="@+id/cp_button_photo"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:layout_weight="0.5"
		    android:drawableLeft="@drawable/ico_foto"
		    android:gravity="center_vertical|left"
		    android:text="   Obter Foto" />

	</LinearLayout>

	<TableLayout
	    android:id="@+id/geoform_tab1"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent"
	    android:layout_weight="0"
	    android:padding="4dp"
	    android:stretchColumns="*">
	    
	    <TableRow
		    android:id="@+id/geoform_tr25"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:gravity="center_vertical"
		    android:weightSum="1" >

			<TextView 
			    android:layout_height="wrap_content"
				android:textColor="@color/black" 
				android:layout_width="fill_parent"
				android:layout_weight="0" 
				android:text="Latitude: " 
				android:id="@+id/cp_latt"/>

			<LinearLayout
			    android:id="@+id/geoform_tr25_lay1"
			    android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:weightSum="1" >

			     <TextView 
			    android:layout_height="wrap_content"
				android:textColor="@color/black" 
				android:layout_width="fill_parent"
				android:layout_weight="0" 
				android:text="" 
				android:id="@+id/cp_lat"/>
			</LinearLayout>

		</TableRow>
	    
	    <TableRow
		    android:id="@+id/geoform_tr26"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:gravity="center_vertical"
		    android:weightSum="1" >

			<TextView 
			    android:layout_height="wrap_content"
				android:textColor="@color/black" 
				android:layout_width="fill_parent"
				android:layout_weight="0" 
				android:text="Longitude: " 
				android:id="@+id/cp_lont"/>

			<LinearLayout
			    android:id="@+id/geoform_tr26_lay1"
			    android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:weightSum="1" >

			     <TextView 
			    android:layout_height="wrap_content"
				android:textColor="@color/black" 
				android:layout_width="fill_parent"
				android:layout_weight="0" 
				android:text="" 
				android:id="@+id/cp_lon"/>
			</LinearLayout>

		</TableRow>
	    

		<TableRow
		    android:id="@+id/tableRow1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content">

			<TextView
			    android:id="@+id/textView1"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:text="Endereço: "
			    android:textColor="@color/black" 
			    android:paddingTop="5dp" 
			    android:paddingBottom="10dp"
			    android:layout_span="2"/>

		</TableRow>

		<TableRow
		    android:id="@+id/geoform_tr1"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:gravity="center_vertical"
		    android:weightSum="1" >

			<TextView 
			    android:text="Logradouro: " 
			    android:layout_height="wrap_content"
				android:textColor="@color/black" 
				android:layout_width="fill_parent"
				android:layout_weight="0" 
				android:id="@+id/cp_logt"/>

			<LinearLayout
			    android:id="@+id/geoform_tr1_lay1"
			    android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:weightSum="1" >

				<AutoCompleteTextView 
				    android:layout_height="wrap_content"
					android:layout_width="fill_parent"
					android:inputType="textPostalAddress" 
					android:maxLength="80"
					android:id="@+id/cp_log" 
					android:textColor="#000" 
					android:layout_weight="1" />
				<ImageButton 
				    android:layout_width="wrap_content" 
				    android:layout_height="wrap_content"
					android:src="@drawable/ico_clear" 
					android:id="@+id/cp_button_clear" 
					android:layout_weight="0"
					android:contentDescription="@string/app_name"/>
			</LinearLayout>
		</TableRow>
		
	<TableRow
	    android:id="@+id/geoform_tr2"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="0"
	    android:gravity="center_vertical"
	    android:weightSum="1" >

		<TextView 
		    android:textColor="@color/black" 
		    android:layout_height="wrap_content" 
		    android:layout_weight="0" 
		    android:layout_width="fill_parent" 
		    android:id="@+id/cp_cept" 
		    android:text="CEP: "/>

		<LinearLayout
		    android:id="@+id/geoform_tr2_lay1"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:layout_weight="1"
		    android:weightSum="1" >

			<EditText 
			    android:layout_width="0dip"
				android:enabled="false" 
				android:layout_weight="1" 
				android:id="@+id/cp_cep"
				android:inputType="number" 
				android:layout_height="wrap_content" 
				android:maxLength="8"/>
		</LinearLayout>
	</TableRow>

	<TableRow
	    android:id="@+id/geoform_tr3"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:gravity="center_vertical"
	    android:weightSum="1" >

			<TextView 
			    android:layout_weight="0" 
			    android:textColor="@color/black"
				android:text="Número: " 
				android:layout_height="wrap_content"
				android:layout_width="fill_parent" 
				android:id="@+id/cp_numt"></TextView>

			<LinearLayout
			    android:id="@+id/geoform_tr3_lay1"
			    android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:weightSum="1" >

				<EditText 
				    android:layout_width="0dip"
					android:id="@+id/cp_num" 
					android:inputType="number" 
					android:layout_height="wrap_content" 
					android:layout_weight="1" >
				</EditText>
			</LinearLayout>
		</TableRow>

	<TableRow
	    android:id="@+id/geoform_tr_stub1"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:gravity="center_vertical"
	    android:weightSum="1" >

	    <TextView
	        android:id="@+id/cp_citt"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="0"
	        android:text="Cidade: "
	        android:textColor="@color/black" />

	    <LinearLayout
	        android:id="@+id/geoform_tr_stub1_lay1"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:weightSum="1" >

	        <EditText
	            android:enabled="false"
	            android:id="@+id/cp_cit"
	            android:layout_width="0dip"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:inputType="number" 
	            android:text="São José dos Campos"/>
	    </LinearLayout>
	</TableRow>

	<TableRow
	    android:id="@+id/geoform_tr_stub2"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:gravity="center_vertical"
	    android:weightSum="1" >

	    <TextView
	        android:id="@+id/cp_estt"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="0"
	        android:text="UF: "
	        android:textColor="@color/black" />

	    <LinearLayout
	        android:id="@+id/geoform_tr_stub2_lay1"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_weight="1"
	        android:weightSum="1" >

	        <EditText
	            android:id="@+id/cp_est"
	            android:enabled="false"
	            android:layout_width="0dip"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:inputType="number"
	            android:text="SP"/>
	    </LinearLayout>
	</TableRow>

	<TableRow
	    android:id="@+id/TableRow01"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content" >

	    <TextView
	        android:id="@+id/TextView01"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_span="2"
	        android:paddingBottom="10dp"
	        android:paddingTop="10dp"
	        android:text="Outras Informações: "
	        android:textColor="@color/black" />
	</TableRow>

		<TableRow
		    android:id="@+id/geoform_tr4"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:gravity="center_vertical"
		    android:weightSum="1" >

			<TextView 
			    android:layout_height="wrap_content"
				android:textColor="@color/black" 
				android:layout_width="fill_parent"
				android:layout_weight="0" 
				android:text="Info 1: " 
				android:id="@+id/cp_if1t"/>

			<LinearLayout
			    android:id="@+id/geoform_tr4_lay1"
			    android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:weightSum="1" >

			     <EditText
	            android:id="@+id/cp_if1"
	            android:enabled="true"
	            android:layout_width="0dip"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text=""/>
			</LinearLayout>

		</TableRow>

		<TableRow
		    android:id="@+id/geoform_tr5"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:gravity="center_vertical"
		    android:weightSum="1" >

			<TextView
			    android:id="@+id/cp_if2t"
			    android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="0"
			    android:text="Info 2:"
			    android:textColor="@color/black" />

			<LinearLayout
			    android:id="@+id/geoform_tr5_lay1"
			    android:layout_width="fill_parent"
			    android:layout_height="wrap_content"
			    android:layout_weight="1"
			    android:weightSum="1" >

			   <EditText
	            android:id="@+id/cp_if2"
	            android:enabled="true"
	            android:layout_width="0dip"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text=""/>

			</LinearLayout>
		</TableRow>

	</TableLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>

	<LinearLayout
	    android:id="@+id/geoform_lay2"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_weight="0"
	    android:background="@color/cinza3"
	    android:paddingTop="3dp"
	    android:weightSum="1" >

		<Button
		    android:id="@+id/cp_button_cancel"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:layout_weight="0.5"
		    android:drawableLeft="@drawable/ico_cancelar"
		    android:gravity="center_vertical|left"
		    android:text="Cancelar" />

		<Button
		    android:id="@+id/cp_button_ok"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:layout_weight="0.5"
		    android:drawableLeft="@drawable/ico_ok"
		    android:gravity="center_vertical|left"
		    android:text="OK" />

	</LinearLayout>

</LinearLayout>