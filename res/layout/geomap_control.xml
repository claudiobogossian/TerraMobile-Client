<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:gravity="bottom"
	android:id="@+id/geomap_control_lay0">

	<FrameLayout android:layout_gravity="right"
		android:layout_margin="10px" android:layout_width="wrap_content" android:id="@+id/geomap_control_lay2" android:layout_height="wrap_content">
	<LinearLayout android:orientation="horizontal" android:id="@+id/geomap_control_lay3" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<ImageButton android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:src="@drawable/ico_voltar"
			android:id="@+id/geomap_control_bt_back"></ImageButton>
		<ImageButton android:layout_height="wrap_content" 
		    android:layout_width="wrap_content" android:src="@drawable/ic_form" 
		    android:id="@+id/geomap_control_bt_form"></ImageButton>
	</LinearLayout>
	</FrameLayout>
</LinearLayout>