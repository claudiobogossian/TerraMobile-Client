package br.org.funcate.baurudigital.tools.extraction.Domain.DAO;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;

import br.org.funcate.baurudigital.tools.extraction.Domain.SourceAddress;

public class SourceAddressDAO {

	public void getAddressBy()
	{
		 try {
			 	Class.forName("net.sourceforge.jtds.jdbc.Driver");
			 
		        Connection con = DriverManager.getConnection("jdbc:jtds:sqlserver://geodb4;instance=sql2012;DatabaseName=Bauru_420","sa","S4geo");
		        Statement stmt = con.createStatement();
		        ResultSet rs = stmt.executeQuery("select * from view_fct_endereco_imovel_bogo");
		        
		        while (rs.next()) {
		        	
		        	SourceAddress address = new SourceAddress(); 
		        			
		        	address.setIdImo(rs.getInt("imo_id"));     
		        	address.setIdLog(rs.getInt("log_id"));
		        	log_id     
		        	log_codlogradouro     
		        	rs.getString("imo_inscricao")     
		        	endereco                                                
		        	numero     
		        	cep       
		        	complemento                                              
		        	estado     
		        	municipio     
		        	bairro                                    
				}
		        
		    } catch(SQLException e) {
		        e.printStackTrace();
		    } catch (ClassNotFoundException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
	}

}
